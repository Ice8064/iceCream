page {
  background: #f6f6f6;
}
.container {
  background: #fff;
}
.xws-list-cell {
  position: relative;
  padding: 34rpx 0;
  margin: 0 30rpx;
  display: flex;
  background: #fff;
  border-bottom: 1rpx solid #ececec;
}
.xws-list-cell__hd {
  flex: 1;
  display: flex;
  align-items: center;
  font-size: 28rpx;
  color: #303231;
}
.xws-field__input {
  flex: 1;
  line-height: 1.6;
  min-height: 44rpx;
  height: auto;
  font-size: 28rpx;
}
.fixed-botton {
  position: fixed;
  bottom: 46rpx;
  left: 28rpx;
  width: 698rpx;
  height: 92rpx;
  line-height: 92rpx;
  border-radius: 8rpx;
  text-align: center;
  color: #fff;
}
